  "type": "library",
  "require": {
    "php": ">=7",
    "moonpie/ipblock": "^1.0",
    "topthink/framework": "^6.0"
  },
  "license": "apache2.0",
  "authors": [
    {
      "name": "chouyee",
      "email": "875010341@qq.com"
    }
  ],
